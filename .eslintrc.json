{
  "env": {
      "browser": true,
      "es2021": true,
      "jest": true
  },
  "extends": "airbnb-base",
  "overrides": [
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "import/no-extraneous-dependencies": [
          "error",
          {"devDependencies": true}
      ],
      "no-restricted-syntax": [
          "error",
          "LabeledStatement",
          "WithStatement"
      ],
      "no-console": "off"
  }
}
  